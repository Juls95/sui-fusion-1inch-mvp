# Ethereum Sepolia Testnet Configuration (Primary Ethereum testnet)
# This uses the main Ethereum testnet which may have better 1inch support
SUI_RPC_URL=https://fullnode.testnet.sui.io
SUI_PRIVATE_KEY=your_sui_testnet_private_key_here

# Ethereum Sepolia testnet settings  
ETH_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
ETH_PRIVATE_KEY=your_ethereum_sepolia_wallet_private_key_here
ONEINCH_API_KEY=your_1inch_api_key_here

# Network configuration for Ethereum Sepolia
NETWORK_NAME=ETHEREUM_SEPOLIA
CHAIN_ID=11155111
USE_REAL_ONEINCH=true

# Gas budget for Sui transactions
GAS_BUDGET=100000000

# This configuration will:
# 1. Use Ethereum Sepolia testnet (Chain ID 11155111) 
# 2. Test with primary Ethereum testnet environment
# 3. May have limited 1inch Fusion+ support (testing needed)
# 4. Zero cost testnet ETH for experiments

